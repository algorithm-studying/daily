   "source": [
    "내풀이\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "8b90bcc3",
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(x):\n",
    "\n",
    "    s = str(x)\n",
    "    add = 0\n",
    "    for i in s:\n",
    "        add += int(i)\n",
    "    if x%add == 0:\n",
    "        return True\n",
    "    else:\n",
    "        return False"
   ]
  },
