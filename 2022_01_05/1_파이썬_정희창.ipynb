    "def solution(array, commands):\n",
    "    q = []\n",
    "    answer = []\n",
    "    for t in commands:\n",
    "        \n",
    "        \n",
    "        answer = array[t[0]-1:t[1]]\n",
    "        answer.sort()\n",
    "        p=answer[t[2]-1]\n",
    "        q.append(p)\n",
    "        \n",
    "    \n",
    "    return q"
   ]
